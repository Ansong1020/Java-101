public class java102_2 {
    private double y;
        private double x;
        
            public double angle() {
                
                double radians = Math.atan2(this.y, this.x);
    
       
        double degrees = Math.toDegrees(radians);
    
       
        if (degrees < 0) {
            degrees += 360;
        }
    
        
        return degrees;
    }
    
}
